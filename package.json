    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crawler": "^1.2.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "node-cron": "^2.0.3",
    "pg": "^8.3.3",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
